  <script>
    $(document).on('ready', function () {
      $.ajax({
        url: "/getMiniCartItems",
        type: "get", 
        data: { 
        },
        success: function(response) {
            var result = JSON.parse(response)
            var res = result.items;
            var price = result.price;
            $('#totalPrice')[0].text = price+'';
            if(!!res.length){
                $('#itemsCount')[0].textContent = res.length+'';
                for(var i=0; i<res.length; i++){
                    $(".mini-cart").append(                
                        `
                        <div data-courseid =` +res[i].id+ ` class="u-basket__product g-brd-none g-px-20">
                            <div class="row no-gutters g-pb-5">
                
                            <div class="col-8">
                                <h6 class="g-font-weight-400 g-font-size-default">
                                <a class="g-color-black g-color-primary--hover g-text-underline--none--hover">`+res[i].title+`</a>
                                </h6>
                                <small class="g-color-primary g-font-size-12">1 x CAD$`+res[i].regular_price+`</small>
                            </div>
                            </div>
                
                        </div>
                    `);
                }

            }
        },
        error: function(xhr) {
            //Do Something to handle error
        }
      });
      $('form').parsley();
    });
